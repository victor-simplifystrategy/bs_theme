{% elsif block.type == 'text_column' %}
    
<div class="gba-text_column" style="background-color:{{block.settings.color}}">
  
  <div class="image">
  	<img src="{{block.settings.image | img_url:'master'}}" />
  </div>
  <div class="content">
    <h2>{{block.settings.title}}</h2>
    <div>
    	{{block.settings.text}}
    </div>
    <a href="{{block.settings.url}}" class="btn">{{block.settings.button_text}}</a>
  </div>
</div>
    
{% endif %}


{% endfor %}
